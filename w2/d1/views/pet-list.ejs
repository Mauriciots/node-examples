<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Pets List</title>
  </head>
  <body>
    <h1>Pets List</h1>
    <ul>
      <% pets.forEach((pet) => {%>
        <%- include("pet-item", { pet }) %>
      <%})%>
    </ul>
    <hr />
    <p>
      <a href="/new">New Pet</a>
    </p>
  </body>
</html>
